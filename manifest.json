{
  "manifest_version": 2,
  "name": "feedit",
  "description": "star articles you like",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/feedit.png",
    "default_title": "feedit"
  },
  "content_scripts": [{
    "js": ["scripts/content.js"],
    "matches": ["http://*/*", "https://*/*"],
    "run_at": "document_start"
  }],
  "permissions": [
    "background",
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["scripts/background.js"]
  }
}

