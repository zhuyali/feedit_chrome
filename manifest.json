{
  "manifest_version": 2,
  "name": "feedit",
  "description": "star articles you like",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/feedit.png",
    "default_title": "feedit",
    "default_popup": "popup.html"
  },
  "permissions": [
    "background",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["scripts/background.js"]
  }
}

